name = 'my-scope/my-profile'
version = '2.0.0'

usecase Test {
    input {
        f1 boolean, 
        f2 string
    }

    result {
        error! Error,
        message! string
    }!
}

model Error enum {
    NOT_FOUND,
    INTERNAL
}

model Message enum {
    SUCCESS,
    FAIL
}

model Data {
    to string
    from string
}

field f1 Data
field f2 Message